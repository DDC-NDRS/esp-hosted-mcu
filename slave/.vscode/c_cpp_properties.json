{
    "configurations": [
        {
            "name": "ESP-IDF",
            "compilerPath": "${config:idf.toolsPathWin}\\tools\\riscv32-esp-elf\\esp-14.2.0_20241119\\riscv32-esp-elf\\bin\\riscv32-esp-elf-gcc.exe",
            "compileCommands": "${config:idf.buildPath}/../build/compile_commands.json",
            "includePath": [
                "C:/toolchain/esp/esp-idf/5.5/components/**",
                "C:/toolchain/esp/esp-idf/5.5/components/esp_driver_i2c/include",
                "C:/toolchain/esp/esp-idf/5.5/components/esp_driver_spi/include",
                "C:/toolchain/esp/esp-idf/5.5/components/esp_event/include",
                "C:/toolchain/esp/esp-idf/5.5/components/esp_system/include",
                "C:/toolchain/esp/esp-idf/5.5/components/esp_wifi/include",
                "C:/toolchain/esp/esp-idf/5.5/components/soc/esp32c6/register",
                "C:/toolchain/esp/esp-idf/5.5/components/riscv/include",
                "C:/toolchain/esp/esp-idf/5.5/components/freertos/FreeRTOS-Kernel/include",
                "C:/toolchain/esp/esp-idf/5.5/components/freertos/FreeRTOS-Kernel/portable/xtensa/include/freertos",
                "${workspaceFolder}/build/config",
                "${workspaceFolder}/**"
            ],
            "browse": {
                "path": [
                    "C:/toolchain/esp/esp-idf/5.5/components/**",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "forcedInclude": [
                "${workspaceFolder}/build/config/sdkconfig.h"
            ],
            "defines": [
                "__GTEST=0",
                "__mcu__=1",
                "CMSIS_OS2_ID=CMSIS_OS2_IS_FREERTOS",
                "CONFIG_SMF_INITIAL_TRANSITION=1",
                "CONFIG_SMF_ANCESTOR_SUPPORT=1"
            ]
        }
    ],
    "version": 4
}
